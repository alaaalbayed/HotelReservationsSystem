@model Domain.Models.LookUpPropertyIndexView

@{
    Layout = "_HotelLayout";
    ViewData["Title"] = "Pretal Hotel";
    ViewData["ActivePage"] = "Index";
}


<!DOCTYPE html>
<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="Sona Template">
    <meta name="keywords" content="Sona, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>PRETAL HOTEAL</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Lora:400,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Cabin:400,500,600,700&display=swap" rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="~/assets/hotel/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/elegant-icons.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/flaticon.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="~/assets/hotel/css/style.css" type="text/css">
</head>

<body>

    <!-- Header Section Begin -->
    <partial name="_HotelNav" />
    <!-- Header End -->
    <!-- Hero Section Begin -->
    <section class="hero-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="hero-text">
                        <h1>Pretal Hotel</h1>
                        <p>
                            Here are the best hotel booking sites, including recommendations for international low priced hotel rooms.
                        </p>
                        <a href="Home/Rooms" class="primary-btn">Discover Now</a>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-5 offset-xl-2 offset-lg-1">
                    <div class="booking-form">
                        <h3>Booking Your Hotel</h3>
                        <form action="#">
                            <div class="check-date">
                                <label for="date-in">Check In:</label>
                                <input type="text" class="date-input" autocomplete="off" id="date-in" required>
                                <i class="icon_calendar"></i>
                            </div>
                            <div class="check-date">
                                <label for="date-out">Check Out:</label>
                                <input type="text" class="date-input" autocomplete="off" id="date-out" required>
                                <i class="icon_calendar"></i>
                            </div>
                            <div class="select-option">
                                <label for="room">Room Type</label>
                                <select id="room">
                                    <option value="" selected disabled>Select a Room Type</option>
                                    @foreach (var room in Model.RoomTypes)
                                    {
                                        <option value="@room.Value">@room.Text</option>
                                    }
                                </select>
                            </div>

                            <button type="submit" asp-action="Rooms" asp-controller="Home">Check Availability</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="hero-slider owl-carousel">
            <img class="hs-item set-bg" src="~/assets/hotel/img/hero/896f954f-0c9f-4029-8322-823d4988af65.jfif">
            <img class="hs-item set-bg" src="~/assets/hotel/img/hero/2023-07-21.jpg">          
        </div>
    </section>
    <!-- Hero Section End -->
    <!-- About Us Section Begin -->
    <section class="aboutus-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="about-text">
                        <div class="section-title">
                            <span>About Us</span>
                            <h2>Pretal Hoteal</h2>
                        </div>
                        <p>
                            بفخر، نحن فندق بريتال، الأول في محافظة المفرق بتصنيف أربع نجوم. نقدم أفضل الخدمات وأشهى المأكولات، ونفتخر بأن نكون الفندق الرائد بهذا التصنيف في إقليم شمال الأردن.
                        </p>
                        <p class="f-para">
                            With pride, we are Pretal Hotel, the first four-star hotel in Al-Mafraq Governorate. We offer the best services and delectable cuisine, proudly being the pioneering hotel with this classification in the Northern Jordan region.
                        </p>
                        <a href="Home/AboutUs" class="primary-btn about-btn">Read More</a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="about-pic">
                        <div class="row">
                            <div class="col-sm-12">
                                <img src="~/assets/hotel/img/about/logo.png" height="400px">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- About Us Section End -->
    <!-- Services Section End -->
    <section class="services-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <span>What We Do</span>
                        <h2>Discover Our Services</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <img src="data:image/png;base64,">
                        <h4>Free parking</h4>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <img src="data:image/png;base64,">
                        <h4>Fitness centre</h4>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <img src="data:image/png;base64,">
                        <h4>Family rooms</h4>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <img src="data:image/png;base64,">
                        <h4>Non-smoking rooms</h4>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <img src="data:image/png;base64,">
                        <h4>Free WiFi</h4>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <img src="data:image/png;base64,">
                        <h4>Room service</h4>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Services Section End -->
    <!-- Home Room Section Begin -->
    <section class="hp-room-section mb-5">
        <div class="container-fluid">
            <div class="hp-room-items">
                <div class="row">
                    @{
                        var roomsToDisplay = Model.Rooms.Take(4);
                        foreach (var room in roomsToDisplay)
                        {
                            <div class="col-lg-3 col-md-6">
                                <div class="hp-room-item set-bg">
                                    @if (room.RoomImages2 != null && room.RoomImages2.Any())
                                    {
                                        <img class="hp-room-item set-bg" src="@room.RoomImages2.First().ImageUrl" alt="">
                                    }
                                    <div class="hr-text mb-6">
                                        <h3>@room.RoomType.NameEn</h3>
                                        <h2>@room.PricePerNight JOD<span>/ Per night</span></h2>
                                        <table>
                                            <tbody>
                                                <tr>
                                                    <td class="r-o">Capacity:</td>
                                                    <td>@room.Capacity</td>
                                                </tr>
                                                <tr>
                                                    <td class="r-o">Services:</td>
                                                    <td>Wifi, Television, Bathroom,...</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <a class="primary-btn" asp-action="RoomDetails" asp-controller="Home" asp-route-id="@room.RoomId" data-delete-url="@Url.Action("RoomDetails", new { id = room.RoomId })">More Details</a>
                                    </div>
                                </div>
                            </div>
                        }
                    }

                </div>
            </div>
        </div>
    </section>
    <!-- Home Room Section End -->
    <!-- Footer Section Begin -->
    <partial name="_HotelFooter" />
    <!-- Footer Section End -->
    <!-- Js Plugins -->
    <script src="~/assets/hotel/js/jquery-3.3.1.min.js"></script>
    <script src="~/assets/hotel/js/bootstrap.min.js"></script>
    <script src="~/assets/hotel/js/jquery.magnific-popup.min.js"></script>
    <script src="~/assets/hotel/js/jquery.nice-select.min.js"></script>
    <script src="~/assets/hotel/js/jquery-ui.min.js"></script>
    <script src="~/assets/hotel/js/jquery.slicknav.js"></script>
    <script src="~/assets/hotel/js/owl.carousel.min.js"></script>
    <script src="~/assets/hotel/js/main.js"></script>

</body>

</html>